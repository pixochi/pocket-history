{
  "expo": {
    "name": "Pocket History",
    "description": "Explore important events from the history, watch videos and read books about them.",
    "slug": "pocket-history",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "version": "2.0.1",
    "orientation": "default",
    "primaryColor": "#0c3441",
    "notification": {
      "icon": "./assets/icons/android.png",
      "color": "#0c3441"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0c3441"
    },
    "ios": {
      "bundleIdentifier": "com.pixo.pockethistory",
      "icon": "./assets/icons/ios.png",
      "supportsTablet": true
    },
    "android": {
      "package": "com.pixo.pockethistory",
      "icon": "./assets/icons/android.png",
      "permissions": ["WAKE_LOCK"],
      "versionCode": 3
    }
  }
}
